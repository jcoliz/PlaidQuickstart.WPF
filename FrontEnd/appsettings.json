{
  "Ui": {
    "WebAddress": "https://standalone:7280/index.html"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "systemd",
      "FormatterOptions": {
        "IncludeScopes": false,
        "UseUtcTimestamp": true,
        "TimestampFormat": " [ dd/MM/yyyy HH:mm:ss ] "
      }
    }
  }
}
